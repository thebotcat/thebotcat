{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-undef": 0,
        "no-redeclare": 0,
        "no-empty": 0,
        "no-setter-return": 0,
        "indent": 0,
        "no-func-assign": 0,
        "no-case-declarations": 0,
        "no-cond-assign": 0
    }
}
